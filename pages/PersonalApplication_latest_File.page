<script>
    $('.select-selected').each(function (i, el) {

        el.addEventListener("focusout", function () {
            // $(this).removeClass('select-arrow-active');
            // $(this).next('.select-items').addClass('select-hide');
            console.log($(this).text());
        });

        el.addEventListener("input", function () {
            var entryCounts = [],
                ths = $(this);

            if (!ths.hasClass('select-arrow-active')) {
                ths.addClass('select-arrow-active');
                ths.parent().find('.select-items').removeClass('select-hide');
            }

            if (ths.text().length > 0 && ths.text() !== ths.parent().find('select').attr('placeholder')) {
                ths.parent().find('.select-items div').each(function (q, opt) {
                    var optionText = $(opt).text().toUpperCase(),
                        // searchString = $(el).text(),
                        searchString = $(el).text().toUpperCase();
                    if (optionText.indexOf(searchString) === -1) {
                        $(opt).hide();
                        $(opt).removeClass('same-as-selected');
                    } else {
                        console.log(entryCounts);
                        entryCounts.push($(opt).text());
                        $(opt).show();
                        $(opt).addClass('same-as-selected');
                    }
                });
                if (entryCounts.length === 1) {
                    ths.parent().find('.select-items').addClass('select-hide');
                    ths.text(entryCounts[0]);
                }
            } else {
                $(this).parent().find('.select-items').each(function (q, opt) {
                    $(opt).show();
                });
            }
        }, false);
    });
</script>

              


