<script src="//unpkg.com/autonumeric"></script>

<script>
    var Overlay = document.querySelector(".popup-overlay");
    var valueBTN = document.querySelectorAll(".block-sellcar__btn");
    var valuePopup = document.querySelector(".value-popup");
    var valueClose = valuePopup.querySelector(".modal-close");
    if (valueBTN && valuePopup) {
        for (var i = 0; i < valueBTN.length; i++) {
            valueBTN[i].addEventListener("click", function (evt) {
                evt.preventDefault();
                valuePopup.classList.add("popup__show");
                Overlay.classList.add("popup-overlay__show");
            });
        }

        valueClose.addEventListener("click", function (evt) {
            evt.preventDefault();
            valuePopup.classList.remove("popup__show");
            Overlay.classList.remove("popup-overlay__show");
        });

        Overlay.addEventListener("click", function (evt) {
            evt.preventDefault();
            valuePopup.classList.remove("popup__show");
            Overlay.classList.remove("popup-overlay__show");
        });
    }
</script>
<script>
    $("input[name*='phone']").each(function (i, el) {
        $(el).mask('(000) 000-0000');
    });
    $('.datepicker-here').each(function (i, el) {
        $(el).mask('00/00/0000');
    });
    $("input.ssn").each(function (i, el) {
        $(el).mask('000-00-0000');
    });
    $(".popup-personal input[name*='payment'], input.income, .popup-personal input[name*='income']").each(function (i, el) {
        var name = $(el).attr('name');
        if (name.indexOf('source') === -1) {
            new AutoNumeric(el, {
                // new AutoNumeric('input[name*=' + name + ']', {
                dotDecimalCharCommaSeparator: ',',
                integerPos: 7,
                integerNeg: 1,
                currencySymbol: '$',
                negativePositiveSignPlacement: 's'
            });
        }
    });
</script>
<script>
    $('.select-selected').each(function (i, el) {

        el.addEventListener("focusout", function () {
            // $(this).removeClass('select-arrow-active');
            // $(this).next('.select-items').addClass('select-hide');
            console.log($(this).text());
        });

        el.addEventListener("input", function () {
            var entryCounts = [],
                ths = $(this);

            if (!ths.hasClass('select-arrow-active')) {
                ths.addClass('select-arrow-active');
                ths.parent().find('.select-items').removeClass('select-hide');
            }

            if (ths.text().length > 0 && ths.text() !== ths.parent().find('select').attr('placeholder')) {
                ths.parent().find('.select-items div').each(function (q, opt) {
                    var optionText = $(opt).text().toUpperCase(),
                        // searchString = $(el).text(),
                        searchString = $(el).text().toUpperCase();
                    if (optionText.indexOf(searchString) === -1) {
                        $(opt).hide();
                        $(opt).removeClass('same-as-selected');
                    } else {
                        console.log(entryCounts);
                        entryCounts.push($(opt).text());
                        $(opt).show();
                        $(opt).addClass('same-as-selected');
                    }
                });
                if (entryCounts.length === 1) {
                    ths.parent().find('.select-items').addClass('select-hide');
                    ths.text(entryCounts[0]);
                }
            } else {
                $(this).parent().find('.select-items').each(function (q, opt) {
                    $(opt).show();
                });
            }
        }, false);
    });
</script>
